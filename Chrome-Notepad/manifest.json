{
"manifest_version": 3,
"name": "Chrome Notepad",
"version": "2.0",
"description": "A Notepad-like extension for chrome!",
"permissions":
[   
    "sidePanel",
    "clipboardRead",
    "clipboardWrite"
],
"icons": 
{
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
},
"side_panel": 
{
      "default_path": "sidePanel.html"
},
"background":
{
    "service_worker": "background.js"
}
}