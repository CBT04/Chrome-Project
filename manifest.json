{
"manifest_version": 3,
"name": "Chrome Notepad",
"version": "1.3",
"description": "A Notepad like extension for chrome!",
"permissions":
[   
    "clipboardRead",
    "clipboardWrite"
],
"icons": 
{
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
},
"action": {
      "default_popup": "popup.html"
}
}